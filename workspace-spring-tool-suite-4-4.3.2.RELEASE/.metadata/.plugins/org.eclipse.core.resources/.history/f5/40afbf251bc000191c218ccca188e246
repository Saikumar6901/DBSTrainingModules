package com.example.demo;

public class Employee {
	private String ename;
	private int age;
	private String designatioon;
	public String getEname() {
		return ename;
	}
	public void setEname(String ename) {
		this.ename = ename;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getDesignatioon() {
		return designatioon;
	}
	public void setDesignatioon(String designatioon) {
		this.designatioon = designatioon;
	}
	@Override
	public String toString() {
		return "Employee [ename=" + ename + ", age=" + age + ", designatioon=" + designatioon + "]";
	}
	

}
