package com.emp;

import com.dept.Department;

public class Emp {
String name;
 int id;
  Department S;
	public Emp() {
		// TODO Auto-generated constructor stub
	}
	public Emp(String name,int id,Department sales) {
		// TODO Auto-generated constructor stub
		this.name=name;
		this.id=id;
		this.S=sales;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public Department getS() {
		return S;
	}
	public void setS(Department sales) {
		this.S= sales;
	}
	public void getDetails() {
		System.out.println(id+" "+name);
		S.deptName();
	}
	public String toString() {
		return id+" "+name+" "+S;
	}
}
