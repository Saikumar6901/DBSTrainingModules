		// TODO Auto-generated constructor stub
		this.x=x;
		this.y=y;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
          
	}
	public void add() {
		int z=x+y;
		System.out.println("in CalcA"+z);
	}

}
