package com.emp;

import com.dept.Department;

public class Emp {
String name;
 int id;
 public Department SALES;
	public Emp() {
		// TODO Auto-generated constructor stub
	}
	public Emp(String name,int id,Department sales) {
		// TODO Auto-generated constructor stub
		this.name=name;
		this.id=id;
		this.SALES=sales;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public Department getSALES() {
		return SALES;
	}
	public void setSALES(Department sales) {
		this.SALES= sales;
	}
	public void getDetails() {
		System.out.println(id+" "+name);
		SALES.deptName();
	}
	public String toString() {
		return id+" "+name+" "+SALES;
	}
}
